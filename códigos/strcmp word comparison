THIS CODE IS ONLY FOR WORDS IT DOES NOT READ PHRASES

#include <stdio.h>
#include <string.h>
int main()
{
    char p1[10] ;
    char p2[10] ;
    
    printf("Enter a word: ");
    scanf(" %s", p1);
    
     printf("Enter a word: ");
    scanf(" %s", p2);


if(strcmp(p1, p2) == 0){
    printf("They are equal");
} else{
    printf("They are not equal");
}
    return 0;
}
